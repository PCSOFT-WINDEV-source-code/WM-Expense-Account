      // Summary: <specify the action of the procedure>
      // Syntax:
      //SelectPhotoInAlbum (<sImagePath>)
      //
      // Parameters:
      // 	sImagePath: <specify the role of sImagePath>
      // Return value:
      // 	None
      
      PROCEDURE SelectPhotoInAlbum(sImagePath)
      
      sPhoto is string = sImagePath
      
      IF fExtractPath(sPhoto,fExtension) ~= ".jpg" THEN
      
      IF NoSpace(sPhoto) <> "" THEN
      	
      	MyWindow..ReturnedValue = sPhoto
      	Close()
      	
      END	
      
      ELSE
      	Info(<§$0000§>)
      	RETURN	
      END
      
