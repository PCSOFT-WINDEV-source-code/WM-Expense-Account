         code : |1+
          
          // Correct input?
          IF EDT_CUSTOMER_NAME ~= "" THEN
          	Error(<§$0002§>)
          	RETURN
          END
          
          // Adds the customer
          HReset(Customer)
          Customer.Name   = EDT_CUSTOMER_NAME
          
          // Adds
          IF HAdd(Customer) = True THEN
          	
          	// Selects the new customer
          	MyWindow..ReturnedValue = Customer.CustomerID
          	
          	Close()
          ELSE
          	Error(HErrorInfo())
          END
          
          
