      // Summary: Refreshes the sort performed in the looper
      // Syntax:
      // RefreshSort ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE RefreshSort()
      
      // Variable
      sCondition is string
      
      SWITCH gnCurrentSort
      	CASE SORT_DATE:
      		sCondition = "ATT_DATE_VALUE"
      	CASE SORT_AMOUNT:
      		sCondition = "ATT_AMOUNT_VALUE"
      	OTHER CASE:
      		Error(<§$0008§>)
      END
      
      SWITCH gnTypeCurrentSort
      	CASE SORT_ASCENDING:
      		sCondition = "+" + sCondition
      	CASE SORT_DESCENDING:
      		sCondition = "-" + sCondition
      	OTHER CASE:
      		Error(<§$000a§>)
      END
      
      LooperSort(sCondition)
