     name : refreshListExpenseAccounts
     internal_properties : 
     procedure_id : 1659525129581420309
     type_code : 14
     code : |1-
      // Summary: Refreshes the list of envelopes
      // Syntax:
      //refreshListExpenseAccounts ([<nExpenseAccountID> [, <nStatusFilter> is int]])
      //
      // Parameters:
      //	nExpenseAccountID (default value=0): Envelope ID
      // 	nStatusFilter (integer - default value=27): <specify the role of nStatus>
      // Return value:
      // 	None
      //
      PROCEDURE refreshListExpenseAccounts(LOCAL nNoteID <useful> = CS_NO_VALUE, nStatusFilter is int = FILTER_STATUS_INPROGRESS)
      
      // Variables
      sAmount   is string
      sDate      is string
      cyAmount  is currency
      cyTotal    is currency
      nExpenseAccountStatus is int 
      
      // Reset the information
      LooperDeleteAll(LOOP_ENVELOPE)
      
      // Configures the query
      QRY_EnvelopeSimpleList.pYear  = Null
      QRY_EnvelopeSimpleList.pMonth   = Null
      
      // Runs the query
      IF HExecuteQuery(QRY_EnvelopeSimpleList) THEN
      	
      	// Browses the query and fill the looper
      	FOR EACH QRY_EnvelopeSimpleList 
      		
      		IF nStatusFilter <> FILTER_STATUS_ALL THEN
      			nExpenseAccountStatus = getStatusForFilter(QRY_EnvelopeSimpleList.EnvelopeID)
      			IF nExpenseAccountStatus <> nStatusFilter THEN
      				CONTINUE
      			END
      		END
      		
      		// Adds the bill into the looper
      		sDate     = StringBuild(<§$000b§>, DateToString(QRY_EnvelopeSimpleList.StartDate,<§$0001§>), DateToString(QRY_EnvelopeSimpleList.EndDate,<§$0001§>))
      		cyAmount = getExpenseAccountAmount(QRY_EnvelopeSimpleList.EnvelopeID)
      		sAmount  = StringBuild(<§$0000§>, NumToString(cyAmount, ",2fS"), pManagingValues.gsSymbolReferenceCurrency)
      		
      				
      		cyTotal += cyAmount
      		
      		// Adds the expense account
      		LooperAddLine(LOOP_ENVELOPE, QRY_EnvelopeSimpleList.EnvelopeID, getImageExpenseAccountStatus(QRY_EnvelopeSimpleList.EnvelopeID), QRY_EnvelopeSimpleList.Name, sDate, sAmount, False, cyAmount, QRY_EnvelopeSimpleList.StartDate, IMG_CheckMarkOff)
      		
      	END
      	
      	EDT_Total = StringBuild(<§$0000§>, NumToString(cyTotal, ",2fS"), pManagingValues.gsSymbolReferenceCurrency)
      	
      ELSE
      	
      	Error(HErrorInfo())
      	
      END
      
      IF LOOP_ENVELOPE..Occurrence = 0 THEN
      	
      	
      	LOOP_ENVELOPE..Visible = False
      	EDT_Total..Visible = False
      	
      	EnableSelectionMenu()
      	OPT_Selection..Visible = False 
      	
      	IF nStatusFilter <> FILTER_STATUS_REIMBURSED THEN
      		SC_No_Bill..X = 0 
      		SC_No_Bill..Width = MyWindow..Width
      		SC_No_Bill..Height = MyWindow..Height
      	END
      	
      ELSE
      	// Sorts the looper
      	RefreshSort()
      	
      	LOOP_ENVELOPE..Visible = True
      	
      	OPT_Selection..Visible = True
      	
      	EDT_Total..Visible = True
      	
      	SC_No_Bill..X = SC_No_Bill..XInitial 
      	
      END
