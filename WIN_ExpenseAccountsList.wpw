      // Summary: Sends by email in HTML format
      PROCEDURE SendHTML()
      
      
      sHTMLFile	is string
      
      <COMPILE IF ConfigurationType=Android> 
      	sHTMLFile = gsAppDir + ["/"] + <§$0016§>
      <ELSE>
      	sHTMLFile  =  CompleteDir(fDataDir()) + <§$0016§>	
      <END>
      
      sHTMLFile = StringBuild(sHTMLFile,Today())
      
      // Variables
      arrEnvelopeID  is array of int
      
      FOR ALL ROW of LOOP_ENVELOPE
      	IF LOOP_ENVELOPE.ATT_CHECK = True THEN
      		ArrayAdd(arrEnvelopeID,LOOP_ENVELOPE.ATT_ID)	
      	END
      END
      
      // Print
      IF arrEnvelopeID..Occurrence > 0 THEN
      	
      	
      	// Check whether invoices are found in the selected expense accounts
      	sIDList is string =  ArrayToString(arrEnvelopeID)
      	QRY_CountExpenseAccounts.pIDList = sIDList
      	HExecuteQuery(QRY_CountExpenseAccounts)
      	IF QRY_CountExpenseAccounts.NbExpenseAccounts = 0 THEN
      		Info(<§@1f2bf341008deb4f0010§>)
      		RETURN
      	END
      	
      	// Reset email
      	EmailReset()
      	
      	sMessage    is string = GetEmailText()
      	
      	
      	// Build the HTML
      	sListPhotos is string
      	sHTML is string = HTMLBuild(arrEnvelopeID, sListPhotos)
      	IF sHTML = "" THEN
      		RETURN
      	END
      	
      	fSaveText(sHTMLFile,sHTML) 
      	
      	
      	Email.Attach[1] = sHTMLFile
      	nSubscript is int = 1	
      	FOR EACH string sPath of sListPhotos SEPARATED BY CR
      		nSubscript++
      		Email.Attach[nSubscript] = sPath
      	END
      	Email.NbAttach       = nSubscript
      	
      	
      	// Builds the email
      	Email.Recipient[1] = GetDefaultEmail()
      	Email.NbRecipient  = 1
      	Email.Subject           = <§$0003§>
      	Email.Message         = sMessage 
      	
      	
      	// Starts the email application
      	EmailRunApp()
      	
      	//Refresh the status >> waiting for reimbursement
      	RefreshStatus(STATUS_PENDING)	
      	
      	EnableSelectionMenu()
      ELSE
      	
      	Info(<§$000e§>)
      	
      END
      
      
      
