     internal_properties : 
     code_elements :
      type_code : 26
      p_codes :
       -
         code : |1+
          
          SWITCH gbModifMode
          
          	CASE False
          		
          		LOOP_ENVELOPE..X +=  CoordinateEditorToScreen(40)
          		
          		MySelf..Caption = <§$000f§>
          		
          		OPT_New..Visible = False
          		
          		OPT_SendByEmail..Visible = True
          		OPT_Print..Visible = True
          
          
          
          		OPT_Delete..Visible = True
