  options :
   -
     name : OPT_SendByEmail
     identifier : 0x2707d150de87df15
     internal_properties : 
     code_elements :
      type_code : 26
      p_codes :
       -
         code : |1+
          bSelection is boolean = False
          
          FOR ALL ROW of LOOP_ENVELOPE
          	IF LOOP_ENVELOPE.ATT_CHECK = True THEN
          		bSelection = True
          		BREAK
          	END
          END
          
          IF bSelection = False THEN
          	Info(<§$000e§>)
          	
          	
          ELSE
          	<COMPILE IF ConfigurationType=iOS>
          		
          		OpenPopupMenu(MENU_Sending)
          		
          	<ELSE>
          		
          		//1: HTML
          		//2: CSV
          		SWITCH Dialog(<§?158D25830736AE5A§>)
          			// HTML
          			CASE 1
          				SendHTML()
          				
          			// CSV
          			CASE 2
          				SendCSV()
          				
          				
          		END
          		
          	<END>
          	
          	
          	
          	
          END
         type : 32
   -
     name : OPT_Print
     identifier : 0x2707d150de88df15
     internal_properties : 
     code_elements :
      type_code : 26
      p_codes :
       -
         code : |1+
          <COMPILE IF ConfigurationType=Android>
          	PrintInHTML()
          <ELSE>
          	PrintInPDF()
          <END>
         type : 32
   -
     name : OPT_Delete
     identifier : 0x2707d150de89df15
     internal_properties : 
     code_elements :
      type_code : 26
      p_codes :
       -
         internal_properties : CAAAAAgAAADPqZ4V/aKAYuvAa87g65ipR5HEkAhRA98r1ncM1HEX77UA5jRzvKEhHL/v8Bwq/Q6b7qLc4NhM/oyjEqd+QjAK81sOXTt2GlpyVIxGdDQa4AGzcrX4bfiZXiZvBm0bqVqbn/mL6GdODsASM2g=
         code : |1+
          
          bDeletion is boolean = False
          
          IF YesNo(<§$000d§>) = Yes THEN
          	FOR ALL ROW of LOOP_ENVELOPE
          		IF LOOP_ENVELOPE.ATT_CHECK = True THEN		
          			IF EnvelopeDelete(LOOP_ENVELOPE.ATT_ID) = False THEN
          				Error(<§$000c§>)
          			ELSE
          				bDeletion = True	
          			END
          		END
          	END
          	
          	// Refreshes the list of envelopes
          	IF bDeletion THEN
          		refreshListExpenseAccounts()
          	END
          END
          
          EnableSelectionMenu()
         type : 32
