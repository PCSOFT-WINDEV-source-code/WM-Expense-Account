  procedures :
   -
     name : CustomerSelect
     internal_properties : 
     procedure_id : 1659525078037475022
     type_code : 14
     code : |1-
      // Summary: Selects a customer in the list
      // Syntax:
      //CustomerSelect (<nCustomerID>)
      //
      // Parameters:
      //	nCustomerID: ID of the customer
      // Return value:
      // 	None
      //
      PROCEDURE CustomerSelect(LOCAL nCustomerID)
      
      
      
      // Selects a customer
      IF nCustomerID <> CS_NO_VALUE THEN
      	// Browse the looper
      	FOR ALL ROW of LOOP_CUSTOMER
      		IF LOOP_CUSTOMER.ATT_ID = nCustomerID THEN
      			LOOP_CUSTOMER.ATT_CHECK = True
      			MyWindow..ReturnedValue = nCustomerID
      			RETURN
      		END
      	END
      END
     type : 458752
