      // Summary: Refreshes the GUI according to the VAT
      // Syntax:
      // RefreshAccordingToVAT ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE RefreshAccordingToVAT()
      
      SWITCH gnCurrentyMode 
      
      	CASE MODE_VAT_UNIQUE 
      		IF MZ_VAT..Occurrence > 2 THEN
      			mlzDelete(MZ_VAT,3)
      		END
      		
      	CASE  MODE_VAT_SECONDARY
      		mlzAddLine(MZ_VAT)
      		IF LocalCurrency.Symbol = "$" THEN
      			MZ_VAT[3].EDT_SECONDARY_VAT..InputMask = "+99 999,99 $" + LocalCurrency.Symbol
      		ELSE
      			MZ_VAT[3].EDT_SECONDARY_VAT..InputMask = "+99 999,99 " + LocalCurrency.Symbol
      		END
      		
      		
      		
      		
      END
      
     type : 458752
   -
     name : invoiceDisplay
     internal_properties : 
     procedure_id : 1659525047969944282
     type_code : 14
     code : |1-
      // Summary: Displays an expense account
      // Syntax:
      //invoiceDisplay (<nInvoiceID> is int)
      //
      // Parameters:
      //	nInvoiceID (integer): Identifier of the expense account
      // Return value:
      // 	None
      //
      PROCEDURE invoiceDisplay(LOCAL nInvoiceID is int)
      
      // Positions on the expense account
      HReadSeekFirst(Invoice,InvoiceID,nInvoiceID)
      IF HFound(Invoice) THEN
      	
      	// Reset
      	ArrayDeleteAll(garrContrib)
      	
      	// Displays the information
      	FileToScreen(WIN_EditInvoice_Tablet, Invoice)
      	
      	// Single VAT or two VATs?
      	IF Val(Invoice.VATAmount2) > 0  THEN
      		gnCurrentyMode = MODE_VAT_SECONDARY
      		BTN_ADD_VAT..BackgroundImage = IMG_MINUS
      		mlzAddLine(MZ_VAT)	
      		MZ_VAT[3].EDT_SECONDARY_VAT = Invoice.VATAmount2
      		MZ_VAT[3].EDT_SECONDARY_VAT_RATE = Invoice.VATRate2
      	ELSE
      		gnCurrentyMode = MODE_VAT_UNIQUE
      		BTN_ADD_VAT..BackgroundImage = IMG_PLUS
      		IF MZ_VAT..Occurrence > 2 THEN
      			mlzDelete(MZ_VAT,3)
      		END
      		
      	END
      
      	// Displays the currency
      	SetCurrency(Invoice.CurrencyID)
      	
      	// Displays the category
      	SetCategory(Invoice.CategoryID)
      	
      	// Customer
      	SetCustomer(Invoice.CustomerID)
      	
      	// Payment
      	SetPayment(Invoice.PaymentTypeID)
      	
      	// Project
      	SetProject(Invoice.ProjectID)
      	
      	// Status
      	SetStatus(Invoice.Status)
      	
      	// Envelope
      	SetExpenseAccount(Invoice.ExpenseAccountID)
      	
      	// Photo?
      	ArrayDeleteAll(gtabPhotos)
      	ArrayDeleteAll(garrDelPhotos)
      	HFilter(Photos,InvoiceID,Invoice.InvoiceID)
      	HReadFirst(Photos,InvoiceID)
      	WHILE NOT HOut(Photos)
      		stAPhoto is stPhoto
      		stAPhoto.nID = Photos.PhotosID
      		stAPhoto.sPath = Photos.Path
      		Add(gtabPhotos,stAPhoto)
      		
      		HReadNext(Photos,InvoiceID)
      	END
      	HDeactivateFilter(Photos)
      	
      	DisplayNbProofs()
      	IF gtabPhotos..Occurrence > 0 THEN
      		gnCurrentSubscript = 1
      	ELSE
      		gnCurrentSubscript = 0	
      	END
      	
      	// Contributors?
      	HFilter(Invoice_Contributors,InvoiceID,Invoice.InvoiceID)
      	HReadFirst(Invoice_Contributors,InvoiceID)
      	WHILE NOT HOut(Invoice_Contributors)
      		Add(garrContrib,Invoice_Contributors.ContributorsID)
      		HReadNext(Invoice_Contributors)
      	END
      	HDeactivateFilter(Invoice_Contributors)
      	
      	SetContributor(garrContrib)
      	
      	// Possibility of deletion
      	OPT_Delete..Visible = True	
      		
      	
      	
      	sSpecialCaption is string
      	nSpecialType is int
      	GetSpecialAccordingToCategory(Invoice.CategoryID, sSpecialCaption, nSpecialType)
      	
      	// Special item?
      	IF sSpecialCaption ~= "" THEN
      		SetSpecial(CS_NO_VALUE)
      	ELSE
      		SetSpecial(Invoice.CategoryID, Invoice.SpecialValue)	
      	END
      	RefreshCommentHeight()
      END
     type : 458752
   -
     name : invoiceReset
     internal_properties : 
     procedure_id : 1659525047970009818
     type_code : 14
     code : |1-
      // Summary: Reinitializes an expense account with the default values
      // Syntax:
      //invoiceReset ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE invoiceReset()
      
      // Reinitializes the controls
      EDIT_IOT = 0
      EDT_VAT     = 0
      EDT_Date    = Today()
      
      // Default values
      SetCategory(CS_NO_VALUE)
      SetCustomer(CS_NO_VALUE)
      SetPayment(gnPaymentTypeID)
      SetProject(CS_NO_VALUE)
      SetStatus(STATUS_TO_SEND)
      SetCurrency(GetDefaultCurrency())
      SetSpecial(CS_NO_VALUE)
      SetExpenseAccount(gnEnvelopeID)
      
      EDT_Date = pTools.GetDefaultDate()
      
      STC_ChooseProofs = StringBuild(<§$0010§>)
      
      ArrayDeleteAll(gtabPhotos)
      ArrayDeleteAll(garrDelPhotos)
      ArrayDeleteAll(garrContrib)
      SetContributor(GetDefaultContributors())
      
      // No deletion is possible, it's a new expense account
      OPT_Delete..Visible = False
      RefreshCommentHeight()
     type : 458752
   -
     name : PhotoAdd
     internal_properties : 
     procedure_id : 1659525047970075354
     type_code : 14
     code : |1+
      // Summary: Adds a photo
      // Syntax:
      //PhotoAdd (<sPhotoPath>)
      //
      // Parameters:
      //	sPhotoPath: Path to the photo
      // Return value:
      // 	None
      //
      PROCEDURE PhotoAdd(LOCAL sPhotoPath)
      
      stAPhoto is stPhoto
      stAPhoto.nID = CS_NO_VALUE
      stAPhoto.sPath = sPhotoPath
      
      // Add a new photo to the array
      Add(gtabPhotos,stAPhoto)
      gnCurrentSubscript = gtabPhotos..Occurrence
      
      DisplayNbProofs()
     type : 458752
   -
     name : PhotoProcess
     internal_properties : 
     procedure_id : 1659525047970140890
     type_code : 14
     code : |1-
      // Summary: Processes the actions that must be done on the photos
      // Syntax:
      //PhotoProcess (<bSave> is boolean)
      //
      // Parameters:
      //	bSave (boolean): Backup?
      // Return value:
      // 	None
      //
      PROCEDURE PhotoProcess(bSave is boolean)
      
      // Cancel?
      IF bSave = False THEN
      	stAPhoto is stPhoto
      	// The files that will not be used must be deleted
      	FOR EACH ELEMENT stAPhoto of gtabPhotos
      		HReadSeekFirst(Photos,Path,stAPhoto.sPath)
      		IF HFound(Photos) = False THEN
      			fDelete(stAPhoto.sPath)
      		END
      	END
      	
      	FOR EACH ELEMENT sFile of garrDelPhotos
      		HReadSeekFirst(Photos,Path,sFile)
      		IF HFound(Photos) = False THEN
      			fDelete(sFile)
      		END	
      	END
      ELSE
      	// Deletes the photos that are no longer requested
      	FOR ALL ELEMENT sFile of garrDelPhotos
      		HReadSeekFirst(Photos,Path,sFile)
      		IF HFound(Photos) = True THEN
      			IF Photos.InvoiceID = gnExpenseAccountID THEN
      				fDelete(sFile)
      			END
      		END	
      	END
      	
      	// Add the new ones
      	FOR ALL ELEMENT stAPhoto of gtabPhotos
      		
      		HReadSeekFirst(Photos,Path,stAPhoto.sPath)
      		IF HFound(Photos) = False THEN
      			HReset(Photos)
      			Photos.Path  = stAPhoto.sPath
      			Photos.InvoiceID = gnExpenseAccountID
      			HAdd(Photos)
      		END
      	END
      END
