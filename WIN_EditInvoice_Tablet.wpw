   -
     name : SetSpecial
     internal_properties : 
     procedure_id : 1659525047970534106
     type_code : 14
     code : |1+
      // Summary: Displays/Removes the special control
      // Syntax:
      //SetSpecial (<nCategoryID> [, <svalue> [, <nSpecialType>]])
      //
      // Parameters:
      //	nCategoryID: Associated category ID
      //	svalue (default value=""): <specify the role of sValue>
      // 	nSpecialType (default value=0): <specify the role of nSpecialType>
      // Return value:
      // 	None
      //
      PROCEDURE SetSpecial(LOCAL nCategoryID, LOCAL svalue = "", LOCAL nSpecialType = 0)
      
      
      // Do we have to hide the special control?
      IF nCategoryID <> CS_NO_VALUE  THEN
      	
      	sSpecialCaption is string
      	GetSpecialAccordingToCategory(nCategoryID, sSpecialCaption, nSpecialType)
      	
      	IF sSpecialCaption ~= "" THEN 
      		//If the row for the special existed, delete it
      		IF  MZ_General..Occurrence = 5 THEN
      			mlzDelete(MZ_General,4)
      		END
      	ELSE
      		
      		//Add the "special"
      		
      		//If the row does not already exist, add it	
      		IF  MZ_General..Occurrence < 5 THEN
      			mlzAddLine(MZ_General)
      		END
      		
      		SWITCH nSpecialType
      			CASE TYPE_NUM 
      				MZ_General[4].EDT_SPECIAL_NUM..Caption = sSpecialCaption
      				MZ_General[4].EDT_SPECIAL_NUM = svalue
      				MZ_General[4].EDT_SPECIAL_NUM..Visible = True
      				MZ_General[4].EDT_SPECIAL_TEXT..Visible = False
      				           
      			OTHER CASE      
      				MZ_General[4].EDT_SPECIAL_TEXT..Caption = sSpecialCaption
      				MZ_General[4].EDT_SPECIAL_TEXT = svalue
      				MZ_General[4].EDT_SPECIAL_NUM..Visible = False
      				MZ_General[4].EDT_SPECIAL_TEXT..Visible = True
      				
      		END
      		
      		
      		
      	END
      ELSE
      	//If the row for the special existed, delete it
      	IF  MZ_General..Occurrence = 5 THEN
      		mlzDelete(MZ_General,4)
      	END
      END
