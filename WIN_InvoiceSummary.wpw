  procedures :
   -
     name : RefreshSynthesis
     internal_properties : 
     procedure_id : 1659525060856290524
     type_code : 14
     code : |1+
      // Summary: Refreshes the preview in synthesis
      // Syntax:
      // RefreshSynthesis ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE RefreshSynthesis()
      
      
      // Displays the month
      STC_CURRENT_MONTH = Upper( StringBuild(<§$0004§>, GetMonthCaption(gdCurrentDate..Month), gdCurrentDate..Year))
      
      refreshTotal()
      
      EDT_TOTAL = StringBuild(<§$0002§>, NumToString(gcyGrandTotal, ",2fS"), pManagingValues.gsSymbolReferenceCurrency)
      EDT_Reimbursements = StringBuild(<§$0003§>, NumToString(gmoAmountReimbursed, ",2fS"), pManagingValues.gsSymbolReferenceCurrency)
      
      
      
      // Calculates the ratio for the progress bar
      IF gmoAmountReimbursed <> 0 _AND_ gcyGrandTotal <> 0 THEN
      	PROGBAR_Repayments = (gmoAmountReimbursed * PROGBAR_Repayments..MaxValue) / gcyGrandTotal	
      	STC_No_Reimbursement..Visible = False
      ELSE
      	PROGBAR_Repayments = 0
      	STC_No_Reimbursement..Visible = True
      END
      
     type : 458752
   -
     name : refreshTotal
     internal_properties : 
     procedure_id : 1659525060856356060
     type_code : 14
     code : |1+
      // Summary: Refreshes the total paid
      // Syntax:
      //refreshTotal ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      
      
      PROCEDURE refreshTotal()
      
      // Variables
      dStartDate is date ; dStartDate = gdCurrentDate
      dEndDate   is date ; dEndDate = gdCurrentDate
      
      
      // Defines the beginning and the end of month
      dStartDate..Day = 1
      dEndDate..Day   = 31
      
      // Reset the information
      gmoAmountReimbursed = 0
      gcyGrandTotal     = 0
      
      // Configures the query
      QRY_ExpenseAccountList.pStartDate   = dStartDate
      QRY_ExpenseAccountList.pEndDate     = dEndDate
      QRY_ExpenseAccountList.pEnvelopeID = Null
      
      // Runs the query
      IF HExecuteQuery(QRY_ExpenseAccountList) THEN
      	
      	// Browses the query and fill the looper
      	FOR EACH QRY_ExpenseAccountList 
      		
      		// Cumulates the total amount
      		gcyGrandTotal += AmountAccordingToReference(QRY_ExpenseAccountList.Amount, QRY_ExpenseAccountList.CurrencyID,QRY_ExpenseAccountList.ExchangeRate)
      		
      		// If the bill was reimbursed, it is cumulated to the total of reimbursed bills
      		IF QRY_ExpenseAccountList.Status = STATUS_REIMBURSED THEN
      			gmoAmountReimbursed += AmountAccordingToReference(QRY_ExpenseAccountList.Amount, QRY_ExpenseAccountList.CurrencyID,QRY_ExpenseAccountList.ExchangeRate)
      		END	
      		
      		END
      	
      ELSE
      	Error(HErrorInfo())
      END
     type : 458752
