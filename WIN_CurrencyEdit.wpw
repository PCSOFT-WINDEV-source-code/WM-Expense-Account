     procedure_id : 1659525108103825411
     type_code : 14
     code : |1-
      // Summary: Displays a specific currency
      // Syntax:
      //CurrencyDisplay (<nIDCurrency>)
      //
      // Parameters:
      // 	nIDCurrency: Currency identifier
      // Return value:
      // 	None
      
      PROCEDURE CurrencyDisplay(LOCAL nCurrencyID)
      
      // Positions on the currency
      HReadSeekFirst(LocalCurrency, CurrencyID, nCurrencyID)
      IF HFound(LocalCurrency) THEN
      
      	FileToScreen(WIN_CurrencyEdit,LocalCurrency)
      	
      ELSE
      	Error(<§$0004§>)
      	Close()	
      END
     type : 458752
   -
     name : CurrencyValidate
     internal_properties : 
     procedure_id : 1659525108103890947
     type_code : 14
     code : |1-
      // Summary: Checks the input
      // Syntax:
      //[ <Result> = ] CurrencyValid ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	boolean: // 	None
      //
      PROCEDURE CurrencyValidate()
      
      IF EDT_CURRENCY_NAME ~= "" THEN
      	Error(<§$0000§>)
      	RESULT False
      END
      
      IF EDT_LOCALCURRENCY_SYMBOL ~= "" THEN
      	Error(<§$0001§>)
      	RESULT False
      END
      
      
      IF gbInitialValue = True AND CBOX_REFERENCE = False THEN
      	Error(<§$0005§>)
      	RESULT False
      END
      
      RESULT True
     type : 458752
   -
     name : CurrencySave
     internal_properties : 
     procedure_id : 1659525108103956483
     type_code : 14
     code : |1+
      // Currency: Saves the current currency
      // Syntax:
      // CurrencySave ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE CurrencySave()
      
      // Edit or addition?
      IF gnCurrencyID <> -1 THEN
      	HReadSeekFirst(LocalCurrency,CurrencyID,gnCurrencyID)
      	IF HFound(LocalCurrency) = False THEN
      		Error(<§$0003§>)
      		Close()
      	END
      ELSE
      	HReset(LocalCurrency)
      END
      
      // Saves the new currency
      ScreenToFile(WIN_CurrencyEdit,LocalCurrency)
      
      // Reference currency? Force to 1
      IF LocalCurrency.Reference = True THEN LocalCurrency.ConversionRate = 1
      
      IF gnCurrencyID <> -1 THEN
      	HModify(LocalCurrency)
      ELSE
      	HAdd(LocalCurrency)
      END
      
      // Saves the identifier of the currency
      gnCurrencyID = LocalCurrency.CurrencyID
      
      // New reference?
      IF LocalCurrency.Reference = True THEN
      	// Remove the previous reference
      	FOR EACH LocalCurrency 
      		IF LocalCurrency.CurrencyID <> gnCurrencyID AND LocalCurrency.Reference = True THEN
      			LocalCurrency.Reference = False
      			HModify(LocalCurrency)	
      		END
      	END
      	
      	pManagingValues.gnReferenceCurrencyID = gnCurrencyID
      	pManagingValues.gsNameReferenceCurrency      = EDT_CURRENCY_NAME
      	pManagingValues.gsSymbolReferenceCurrency  = EDT_LOCALCURRENCY_SYMBOL
      END
      
      
      MyWindow..ReturnedValue = gnCurrencyID
      Close()
      
      
     type : 458752
