  procedures :
   -
     name : RefreshPaymentList
     internal_properties : 
     procedure_id : 1659525099513299370
     type_code : 14
     code : |1-
      // Summary: Refreshes the list of statuses
      // Syntax:
      //RefreshPaymentList (<nIDPaymentType>)
      //
      // Parameters:
      //	nIDPaymentType: <specify the role of nPaymentValue>
      // Return value:
      // 	None
      //
      PROCEDURE RefreshPaymentList(LOCAL nPaymentTypeID)
      
      // Variable
      sSearchItem is string
      nSubscript           is int = 0
      
      // Reset
      LooperDeleteAll(LOOP_PAYMENT)
      
      // Defines the search item
      SWITCH Nation()
      	CASE nationFrench:
      		sSearchItem = PaymentType.Caption..Name
      	OTHER CASE:
      		sSearchItem = PaymentType.CaptionUS..Name
      END
      
      // Displays the categories
      HReadFirst(PaymentType,sSearchItem)
      WHILE NOT HOut(PaymentType)
      	
      	// Fills the looper
      	IF ExtractString(HInfoMemo(PaymentType,Picto),3) > 0 THEN
      		nSubscript = LooperAddLine(LOOP_PAYMENT, {PaymentType..Name + "." + sSearchItem,indItem}, PaymentType.PaymentTypeID, False, PaymentType.Picto)
      	ELSE
      		nSubscript = LooperAddLine(LOOP_PAYMENT, {PaymentType..Name + "." + sSearchItem,indItem}, PaymentType.PaymentTypeID, False, IMG_PaymentCash)
      	END
      	// Category to select?
      	IF PaymentType.PaymentTypeID = nPaymentTypeID THEN
      		LOOP_PAYMENT[nSubscript].ATT_CHECK = True
      		MyWindow..ReturnedValue = nPaymentTypeID
      	END
      	
      	// Next
      	HReadNext(PaymentType, sSearchItem)
      END
     type : 458752
   -
     name : PaymentTypeSelect
     internal_properties : 
     procedure_id : 1659525099513364906
     type_code : 14
     code : |1+
      // Summary: Selects a type of payment in the list
      // Syntax:
      //PaymentTypeSelect (<nPaymentTypeID>)
      //
      // Parameters:
      //	nPaymentTypeID: ID of the payment type
      // Return value:
      // 	None
      //
      PROCEDURE PaymentTypeSelect(LOCAL nPaymentTypeID)
      
      // Selects the type of payment
      IF nPaymentTypeID <> CS_NO_VALUE THEN
      	
      	// Browse the looper
      	FOR ALL ROW of LOOP_PAYMENT
      		IF LOOP_PAYMENT.ATT_ID = nPaymentTypeID THEN
      			LOOP_PAYMENT.ATT_CHECK = True
      			RETURN
      		END
      	END
      	
      END
      
     type : 458752
