   -
     internal_properties : CAAAAAgAAAC1DjdUr72aENYkbymrX8MzBPS/qeMf6IjPaHZbJjnPdvlc54ez/pgqIYEgB9dAsFSNrdhJgnp63a12zrhAAg1Pu1drmqsdbuAofI7F+jicVFOR593s2ui/Q4cuq528qZ5OqQ19ODaepWfXLw5raYXMW+4H/GtkaijfL+6CoBBzYYQh/pWSPqlGmdmTTomr/Dg1skLMDXzpBLSI6NLYBY6QoQ==
     code : |1+
      
      SWITCH MyChildWindow..Name
      
      	CASE "WIN_InvoiceEdit" :
      		
      		EnvelopeRefresh()			
      		
      		//Reposition on the added or modified invoice
      		nSubscript is int = LooperSeek(ATT_ID, MyChildWindow..ReturnedValue)
      		//The invoice is not found in the list, therefore it is a new invoice, add it... 
      		IF nSubscript <> -1 THEN
      			LooperPosition(LOOP_EXPENSE_ACCOUNT, nSubscript)
      		END
      		
      	CASE  "WIN_StatusSelection" :
      		
      		nStatusValue is int = MyChildWindow..ReturnedValue
      		
      		FOR ALL ROW n of LOOP_EXPENSE_ACCOUNT
      			// Modifies the value of the status
      			HReadSeekFirst(Invoice,InvoiceID,ATT_ID[n])
      			IF HFound(Invoice) THEN
      				// Modifies in database
      				Invoice.Status = nStatusValue
      				HModify(Invoice)
      			END
      		END
      		
      	OTHER CASE
      		
      END
      
