  procedures :
   -
     name : EnvelopeRefresh
     internal_properties : 
     procedure_id : 1659525060856619157
     type_code : 14
     code : |1-
      // Summary: Refreshes the list of the envelope
      // Syntax:
      //EnvelopeRefresh ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE EnvelopeRefresh()
      
      // Variables
      sCaption is string
      sDate    is string
      sAmount is string
      cyTotal    is currency
      
      // Reset
      LooperDeleteAll(LOOP_EXPENSE_ACCOUNT)
      
      // Positions on the envelope
      HReadSeekFirst(Expenses,ExpenseAccountID,gnEnvelopeID)
      IF HFound(Expenses) THEN
      	
      	// List of expense accounts included in the envelope
      	QRY_ExpenseAccountList.pEnvelopeID = Expenses.ExpenseAccountID
      	QRY_ExpenseAccountList.pStartDate   = Null
      	QRY_ExpenseAccountList.pEndDate     = Null
      	
      	// Runs the query
      	IF HExecuteQuery(QRY_ExpenseAccountList) = True THEN
      		FOR EACH QRY_ExpenseAccountList 
      			// Defines the caption
      			IF NoSpace(QRY_ExpenseAccountList.Caption) ~= "" THEN
      				
      				// In this case, displays the caption of the category
      				SWITCH Nation()
      					CASE nationFrench:
      						sCaption = QRY_ExpenseAccountList.Name
      					OTHER CASE:
      						sCaption = QRY_ExpenseAccountList.NameUS
      				END
      				
      			ELSE
      				sCaption = NoSpace(QRY_ExpenseAccountList.Caption)
      			END
      			
      			sDate    = DateToString(QRY_ExpenseAccountList.DATE,<§$0001§>)
      			sAmount = StringBuild(<§$0002§>, NumToString(QRY_ExpenseAccountList.Amount, ",2fS"), QRY_ExpenseAccountList.Symbol)
      			
      			//If the invoice currency is not the reference currency, display the conversion in the reference currency
      			IF QRY_ExpenseAccountList.CurrencyID <> pManagingValues.gnReferenceCurrencyID THEN
      				cyConversion is currency = AmountAccordingToReference(QRY_ExpenseAccountList.Amount, QRY_ExpenseAccountList.CurrencyID, QRY_ExpenseAccountList.ExchangeRate) 
      								
      				sAmount += " (" + NumToString(Round(cyConversion, 2),",2fS") + [" "] + pManagingValues.gsSymbolReferenceCurrency + ")"
      				
      				IF QRY_ExpenseAccountList.BankCharges <> 0 THEN
      					sAmount +=  <§$000c§> + NumToString(QRY_ExpenseAccountList.BankCharges, ",2fS") + [" "] + pManagingValues.gsSymbolReferenceCurrency
      				END
      			END
      			
      			cyTotal += AmountAccordingToReference(QRY_ExpenseAccountList.Amount, QRY_ExpenseAccountList.CurrencyID, QRY_ExpenseAccountList.ExchangeRate) + QRY_ExpenseAccountList.BankCharges
      			
      			IF ExtractString(HInfoMemo(QRY_ExpenseAccountList,Picto),3) > 0 THEN
      				LooperAddLine(LOOP_EXPENSE_ACCOUNT, QRY_ExpenseAccountList.ExpenseID, QRY_ExpenseAccountList.Picto, sCaption, sDate, sAmount, False, QRY_ExpenseAccountList.Amount)	 
      			ELSE
      				LooperAddLine(LOOP_EXPENSE_ACCOUNT, QRY_ExpenseAccountList.ExpenseID, IMG_Various, sCaption, sDate, sAmount, False, QRY_ExpenseAccountList.Amount)	 
      			END
      			
      		END
      		
      		EDT_Total = StringBuild(<§$0008§>,NumToString(cyTotal, ",2fS"), pManagingValues.gsSymbolReferenceCurrency)
      		
      	END
      	
      ELSE
      	Error(<§$0000§>)
      	Close()
      END
      
      IF LOOP_EXPENSE_ACCOUNT..Occurrence = 0 THEN
      	STC_NO_INVOICE..Visible = True
      	
      	BTN_DATE..Visible = False
      	IMG_sort_date..Visible = False
      	
      	BTN_AMOUNT..Visible = False
      	IMG_sort_amount..Visible = False
      	
      	EDT_Total..Visible = False
      	
      ELSE
      	STC_NO_INVOICE..Visible = False
      	
      	BTN_DATE..Visible = True
      	IMG_sort_date..Visible = True
      	
      	BTN_AMOUNT..Visible = True
      	IMG_sort_amount..Visible = True
      	EDT_Total..Visible = True
      	
      END
