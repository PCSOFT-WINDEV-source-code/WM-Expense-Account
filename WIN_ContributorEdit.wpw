         code : |1+
          
          
          // Correct input?
          IF EDT_CONTRIBUTOR_NAME ~= "" THEN
          	Error(<§$0000§>)
          	RETURN
          END
          
          // Does the contributor already exist?
          HReadSeekFirst(Contributors,Name,EDT_CONTRIBUTOR_NAME)
          IF HFound(Contributors) THEN
          	Error(<§$0003§>)
          	RETURN
          END
          
          // Adds the contributor
          HReset(Contributors)
          Contributors.Name = EDT_CONTRIBUTOR_NAME
          
          // Adds
          IF HAdd(Contributors) = True THEN
          	
          	MyWindow..ReturnedValue = Contributors.ContributorsID
          	
          	// Closes the window 
          	Close()
          	
          ELSE
          	Error(HErrorInfo())
          END
