   -
     name : refreshSelection
     internal_properties : 
     procedure_id : 1659525142469470738
     type_code : 14
     code : |1-
      // Summary: Refreshes the selection
      // Syntax:
      //refreshSelection (<nEnveloppeID>)
      //
      // Parameters:
      // 	nEnvelopeID: Envelope to select
      // Return value:
      // 	None
      //
      PROCEDURE refreshSelection(LOCAL nEnvelopeID)
      
      // Variables
      nSubscript is int
      
      // Deselects the previous one
      IF gnCurrentSelection <> CS_NO_VALUE THEN
      	nSubscript = LooperSeek("LOOP_ENVELOPE.ATT_ID",gnCurrentSelection)
      	IF nSubscript <> -1 THEN
      		LOOP_ENVELOPE.ATT_BAR_VISIBLE[nSubscript] = False
      	END
      END
      
      // New selection
      nSubscript = LooperSeek("LOOP_ENVELOPE.ATT_ID",nEnvelopeID)
      
      IF nSubscript <> -1 THEN
      	gnCurrentSelection = nEnvelopeID
      	LOOP_ENVELOPE.ATT_BAR_VISIBLE[nSubscript] = True	
      	
      	IF gbModifMode = True THEN
      		LOOP_ENVELOPE.ATT_CHECK = NOT  LOOP_ENVELOPE.ATT_CHECK
      		IF LOOP_ENVELOPE.ATT_CHECK = True THEN
      			LOOP_ENVELOPE.ATT_CHECKMARK_IMAGE = IMG_CheckMarkOn
      		ELSE
      			LOOP_ENVELOPE.ATT_CHECKMARK_IMAGE = IMG_CheckMarkOff
      		END
      	END
      	
      END
      
      refreshBillTotal()
