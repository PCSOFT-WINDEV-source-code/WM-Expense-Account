   -
     name : refreshBillStatus
     internal_properties : 
     procedure_id : 1659525142469339666
     type_code : 14
     code : |1+
      // Summary: New status for the entire envelope
      // Syntax:
      //refreshBillStatus (<nStatusValue>)
      //
      // Parameters:
      //	nStatusValue: New value
      // Return value:
      // 	None
      //
      PROCEDURE refreshBillStatus(LOCAL nStatusValue)
      
      // Browses the looper
      FOR ALL ROW of LOOP_ENVELOPE
      	IF LOOP_ENVELOPE.ATT_CHECK = True THEN
      		// Retrieves all the expense accounts for this envelope
      		QRY_ExpenseAccountList.pStartDate   = Null
      		QRY_ExpenseAccountList.pEndDate     = Null
      		QRY_ExpenseAccountList.pEnvelopeID = LOOP_ENVELOPE.ATT_ID
      		
      		IF HExecuteQuery(QRY_ExpenseAccountList) THEN
      			FOR EACH QRY_ExpenseAccountList 
      				// Modifies the value of the status
      				HReadSeekFirst(Invoice,InvoiceID,QRY_ExpenseAccountList.ExpenseID)
      				IF HFound(Invoice) THEN
      					// Modifies in database
      					Invoice.Status = nStatusValue
      					HModify(Invoice)
      				END
      			END
      			
      			LOOP_ENVELOPE.ATT_IMG_STATUS = getImageExpenseAccountStatus(LOOP_ENVELOPE.ATT_ID)
      			LOOP_ENVELOPE.ATT_CHECK      = False
      		END
      	END
      END
      
      
      
      
      
      
