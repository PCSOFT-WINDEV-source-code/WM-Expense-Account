   -
     name : refreshInvoiceStatus
     internal_properties : 
     procedure_id : 1659525142469405202
     type_code : 14
     code : |1+
      // Summary: New status for all the bills
      // Syntax:
      //refreshInvoiceStatus (<nStatusValue>)
      //
      // Parameters:
      //	nStatusValue: New value
      // Return value:
      // 	None
      //
      PROCEDURE refreshInvoiceStatus(LOCAL nStatusValue)
      
      // Browses the looper
      FOR ALL ROW of LOOP_EXPENSE_ACCOUNT
      	IF LOOP_EXPENSE_ACCOUNT.ATT_CHECK = True THEN
      
      		// Modifies the value of the status
      		HReadSeekFirst(Invoice,InvoiceID,QRY_ExpenseAccountList.ExpenseID)
      		IF HFound(Invoice) THEN
      			// Modifies in database
      			Invoice.Status = nStatusValue
      			HModify(Invoice)
      			
      			LOOP_EXPENSE_ACCOUNT.ATT_IMG_STATUS = getImageAccordingToStatus(nStatusValue)
      			LOOP_EXPENSE_ACCOUNT.ATT_CHECK = False
      		END
      	END
      END
      
      RefreshSynthesis()
      
      
      
