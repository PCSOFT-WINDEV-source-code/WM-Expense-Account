   -
     name : SendEmailCSV
     internal_properties : 
     procedure_id : 1659525142469536274
     type_code : 14
     code : |1+
      // Summary: <specify the action of the procedure>
      // Syntax:
      //SendEmailCSV ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE SendEmailCSV()
      
      // Variables
      arrListExpenseAccounts is array of int
      sFileCSV is string = CompleteDir(fDataDir()) + <§$000a§>
      sFileName is string
      sContentCSV is string
      sMessage    is string = GetEmailText()
      nSubscript     is int  = 0
      sListPhotos is string
      
      // Reset email
      EmailReset()
      
      // Build the path
      sFileCSV = StringBuild(sFileCSV,Today())
      
      // Builds the files
      FOR ALL ROW of LOOP_ENVELOPE
      	
      	IF LOOP_ENVELOPE.ATT_CHECK = True THEN
      		nSubscript++
      		
      		// Reset array
      		ArrayDeleteAll(arrListExpenseAccounts)
      		
      		// Retrieves all the expense accounts for this envelope
      		QRY_ExpenseAccountList.pStartDate   = Null
      		QRY_ExpenseAccountList.pEndDate     = Null
      		QRY_ExpenseAccountList.pEnvelopeID = LOOP_ENVELOPE.ATT_ID
      		IF HExecuteQuery(QRY_ExpenseAccountList) THEN
      			FOR EACH QRY_ExpenseAccountList 
      				Add(arrListExpenseAccounts, QRY_ExpenseAccountList.ExpenseID)
      				
      				HReadSeekFirst(Photos,InvoiceID,QRY_ExpenseAccountList.ExpenseID)
      				WHILE HFound(Photos)
      					sListPhotos += [CR] + Photos.Path
      					HReadNext(Photos)
      				END
      				
      			END
      		END
      		
      		
      		// Check whether invoices are found in the selected expense accounts
      		IF arrListExpenseAccounts..Occurrence = 0 THEN
      			Info(<§$0011§>)
      			RETURN
      		END
      		
      		// Builds the file
      		sContentCSV = EmailBuild(arrListExpenseAccounts)
      		sFileName = StringBuild(sFileCSV,LOOP_ENVELOPE.ATT_ID)
      		fSaveText(sFileName,sContentCSV)
      		Email.Attach[nSubscript] = sFileName
      		
      		
      	END
      END
      
      // Prepares the email
      IF nSubscript > 0 THEN
      	
      	// Builds the email
      	Email.Recipient[1] = GetDefaultEmail()
      	Email.NbRecipient  = 1
      	Email.Subject           = <§$000d§>
      	Email.Message         = sMessage
      	
      	FOR EACH string sPath of sListPhotos SEPARATED BY CR
      		nSubscript++
      		Email.Attach[nSubscript] = sPath
      	END
      	
      	Email.NbAttach       = nSubscript
      	
      	// Starts the email application
      	EmailRunApp()
      	
      	//Refresh the status >> waiting for reimbursement
      	refreshBillStatus(STATUS_PENDING)	
      	
      ELSE
      	Info(<§$000e§>)
      END
      
      
      
