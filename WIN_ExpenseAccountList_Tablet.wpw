     name : refreshListExpenseAccounts
     internal_properties : 
     procedure_id : 1659525142468684306
     type_code : 14
     code : |1+
      // Summary: Refreshes the list of envelopes
      // Syntax:
      //refreshListExpenseAccounts ([<nEnvelopeID> [, <nStatusFilter> is int]])
      //
      // Parameters:
      //	nEnvelopeID (default value=0): Envelope ID
      // 	nStatusFilter (integer - default value=27): <specify the role of nStatusFilter>
      // Return value:
      // 	None
      //
      PROCEDURE refreshListExpenseAccounts(LOCAL nEnvelopeID = CS_NO_VALUE, nStatusFilter is int = FILTER_STATUS_INPROGRESS) 
      
      // Variables
      sAmount   is string
      sDate      is string
      cyAmount  is currency
      cyTotal    is currency
      nFinalSubscript is int = 1
      nExpenseAccountStatus is int 
      
      // Reset the information
      LooperDeleteAll(LOOP_ENVELOPE)
      
      // Configures the query
      QRY_EnvelopeSimpleList.pYear  = Null
      QRY_EnvelopeSimpleList.pMonth   = Null
      
      // Runs the query
      IF HExecuteQuery(QRY_EnvelopeSimpleList) THEN
      	
      	// Browses the query and fill the looper
      	FOR EACH QRY_EnvelopeSimpleList 
      		
      		IF nStatusFilter <> FILTER_STATUS_ALL THEN
      			nExpenseAccountStatus = getStatusForFilter(QRY_EnvelopeSimpleList.EnvelopeID)
      			IF nExpenseAccountStatus <> nStatusFilter THEN
      				CONTINUE
      			END
      		END
      		
      		// Adds the bill into the looper
      		sDate     = StringBuild(<§$0004§>, DateToString(QRY_EnvelopeSimpleList.StartDate,<§$0005§>), DateToString(QRY_EnvelopeSimpleList.EndDate,<§$0005§>))
      		cyAmount = getEnvelopeAmount(QRY_EnvelopeSimpleList.EnvelopeID)
      		sAmount  = StringBuild(<§$0003§>, NumToString(cyAmount, ",2fS"), pManagingValues.gsSymbolReferenceCurrency)
      		
      		cyTotal += cyAmount
      		
      		// Adds the expense account
      		LooperAddLine(LOOP_ENVELOPE, QRY_EnvelopeSimpleList.EnvelopeID, getImageExpenseAccountStatus(QRY_EnvelopeSimpleList.EnvelopeID), QRY_EnvelopeSimpleList.Name, sDate, sAmount, False, cyAmount,QRY_EnvelopeSimpleList.StartDate, False, IMG_CheckMarkOff)
      	END
      	
      ELSE
      	Error(HErrorInfo())
      END
      
      IF LOOP_ENVELOPE..Occurrence <> 0 THEN
      	
      	// Sorts the looper
      	RefreshSort(LOOP_ENVELOPE..Name,gnCurrentSortEnv,gnTypeCurrentSortEnv)
      	
      	// Any particular subscript?
      	IF nEnvelopeID = CS_NO_VALUE THEN
      		// Selects the first row by default
      		LOOP_ENVELOPE = 1
      		LOOP_ENVELOPE.ATT_BAR_VISIBLE[1] = True
      		gnCurrentSelection = LOOP_ENVELOPE.ATT_ID[1]
      	ELSE
      		refreshSelection(nEnvelopeID)
      	END
      	
      	GR_NEW_EXPENSEACCOUNT..Visible = False
      	
      	
      	BTN_Selection..Visible = True
      	IF BTN_Selection..Caption = <§$0014§> THEN
      		GR_ACTIONS_EXPENSEACCOUNT..Visible = True	
      	END
      	
      	BTN_ADD_BILL..Visible = True
      ELSE	
      	
      	BTN_ADD_BILL..Visible = False
      	IF nStatusFilter <> FILTER_STATUS_REIMBURSED THEN
      		GR_NEW_EXPENSEACCOUNT..Visible = True
      	END
      	
      	
      	BTN_DELETE_ENVELOPE..Visible = False
      	BTN_MODIFY_ENVELOPE_STATUS..Visible = False
      	BTN_EMAIL_ENVELOPE..Visible = False
      	BTN_PRINT_ENVELOPE..Visible = False
      	
      	BTN_Selection..Visible = False
      END
      
      refreshBillTotal()
      
      
     type : 458752
   -
     name : refreshListInvoices
     internal_properties : 
     procedure_id : 1659525142468749842
     type_code : 14
     code : |1+
      // Summary: Refreshes the list of bills
      // Syntax:
      //refreshListInvoices ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE refreshListInvoices()
      
      // Variables
      dStartDate is date ; dStartDate = gdCurrentDate
      dEndDate   is date ; dEndDate = gdCurrentDate
      sAmount   is string
      sDate      is string
      sCaption   is string
      
      // Defines the beginning and the end of month
      dStartDate..Day = 1
      dEndDate..Day   = 31
      
      // Reset the information
      LooperDeleteAll(LOOP_EXPENSE_ACCOUNT)
      
      // Configures the query
      QRY_ExpenseAccountList.pStartDate   = Null
      QRY_ExpenseAccountList.pEndDate     = Null
      QRY_ExpenseAccountList.pEnvelopeID = gnCurrentSelection
      
      // Runs the query
      IF HExecuteQuery(QRY_ExpenseAccountList) THEN
      	
      	// Browses the query and fill the looper
      	FOR EACH QRY_ExpenseAccountList 
      		// Adds the bill into the looper
      		sDate    = DateToString(QRY_ExpenseAccountList.DATE,<§$0005§>)
      		
      		
      		// Defines the caption
      		IF NoSpace(QRY_ExpenseAccountList.Caption) = "" THEN
      			
      			// In this case, displays the caption of the category
      			SWITCH Nation()
      				CASE nationFrench:
      					sCaption = QRY_ExpenseAccountList.Name
      				OTHER CASE:
      					sCaption = QRY_ExpenseAccountList.NameUS
      			END
      			
      		ELSE
      			sCaption = NoSpace(QRY_ExpenseAccountList.Caption)
      		END
      		
      		sAmount = StringBuild("%1 %2", NumToString(QRY_ExpenseAccountList.Amount, ",2fS"), QRY_ExpenseAccountList.Symbol)
      		
      		//If the invoice currency is not the reference currency, display the conversion in the reference currency
      		IF QRY_ExpenseAccountList.CurrencyID <> pManagingValues.gnReferenceCurrencyID THEN
      			
      			cyConversion is currency = AmountAccordingToReference(QRY_ExpenseAccountList.Amount, QRY_ExpenseAccountList.CurrencyID, QRY_ExpenseAccountList.ExchangeRate) 			
      			sAmount += "(" + NumToString(Round(cyConversion, 2),",2fS") + [" "] + pManagingValues.gsSymbolReferenceCurrency + ")"
      			
      			IF QRY_ExpenseAccountList.BankCharges <> 0 THEN
      				sAmount += <§$0010§> + NumToString(QRY_ExpenseAccountList.BankCharges, ",2fS") + [" "] + pManagingValues.gsSymbolReferenceCurrency
      			END
      		END
      		
      		
      		
      		IF ExtractString(HInfoMemo(QRY_ExpenseAccountList,Picto),3) > 0 THEN
      			LooperAddLine(LOOP_EXPENSE_ACCOUNT, QRY_ExpenseAccountList.ExpenseID, QRY_ExpenseAccountList.Picto, sCaption, sDate, sAmount, QRY_ExpenseAccountList.Amount, False, QRY_ExpenseAccountList.CurrencyID, QRY_ExpenseAccountList.ExchangeRate, QRY_ExpenseAccountList.BankCharges)	 
      		ELSE
      			LooperAddLine(LOOP_EXPENSE_ACCOUNT, QRY_ExpenseAccountList.ExpenseID, IMG_Various, sCaption, sDate, sAmount, QRY_ExpenseAccountList.Amount, False,QRY_ExpenseAccountList.CurrencyID, QRY_ExpenseAccountList.ExchangeRate, QRY_ExpenseAccountList.BankCharges)	 
      		END
      		
      		
      	END
      	
      ELSE
      	Error(HErrorInfo())
      END
      
      IF LOOP_EXPENSE_ACCOUNT..Occurrence = 0 THEN
      	
      	STC_NO_INVOICE..Visible = True
      	
      	BTN_SORT_BILL_DATE..Visible = False
      	IMG_sort_date..Visible = False
      	
      	BTN_SORT_BILL_AMOUNT..Visible = False
      	IMG_sort_amount..Visible = False
      	
      ELSE
      	
      	STC_NO_INVOICE..Visible = False
      	
      	BTN_SORT_BILL_DATE..Visible = True
      	IMG_sort_date..Visible = True
      	
      	BTN_SORT_BILL_AMOUNT..Visible = True
      	IMG_sort_amount..Visible = True
      	
      	
      END
      
      refreshTotalInvoices()
      
     type : 458752
   -
     name : getImageExpenseAccountStatus
     internal_properties : 
     procedure_id : 1659525142468815378
     type_code : 14
     code : |1-
      // Summary: Retrieves the image according to the status
      // Syntax:
      //[ <Result> = ] getImageExpenseAccountStatus (<nExpenseAccountID>)
      //
      // Parameters:
      //	nExpenseAccountID: Status value
      // Return value:
      // 	variant: 
      //
      PROCEDURE getImageExpenseAccountStatus(LOCAL nNoteID)
      
      // Variable
      aaStatus is ASSOCIATIVE ARRAY of booleans
      
      // Retrieves the statuses
      HFilter(Invoice,ExpenseAccountID,nNoteID)
      HReadFirst(Invoice,ExpenseAccountID)
      WHILE NOT HOut(Invoice)
      	aaStatus[Invoice.Status] = True
      	HReadNext(Invoice,ExpenseAccountID)
      END
      HDeactivateFilter(Invoice)
      
      // Penalization order
      IF aaStatus[STATUS_REFUSED]..Empty = False THEN
      	RESULT IMG_REFUSED
      END
      
      IF aaStatus[STATUS_TO_SEND]..Empty = False THEN
      	RESULT IMG_WAIT_SENDING
      END
      
      IF aaStatus[STATUS_PENDING]..Empty = False THEN
      	RESULT IMG_WAITING_FOR_REIMBURSEMENT
      END
      
      IF aaStatus[STATUS_REIMBURSED]..Empty = False THEN
      	RESULT IMG_REIMBURSED
      END
      
      RESULT IMG_EMPTY
     type : 458752
   -
     name : getEnvelopeAmount
     internal_properties : 
     procedure_id : 1659525142468880914
     type_code : 14
     code : |1+
      // Summary: Returns the amount of an envelope
      // Syntax:
      //[ <Result> = ] getEnvelopeAmount (<nEnvelopeID>)
      //
      // Parameters:
      //	nEnvelopeID: Envelope ID
      // Return value:
      // 	currency: // 	None
      //
      
      PROCEDURE getEnvelopeAmount(LOCAL nEnvelopeID)
      
      // Variable
      cyAmount is currency
      
      // Retrieves the statuses
      HFilter(Invoice,ExpenseAccountID,nEnvelopeID)
      HReadFirst(Invoice,ExpenseAccountID)
      WHILE NOT HOut(Invoice)
      	cyAmount += AmountAccordingToReference(Invoice.Amount, Invoice.CurrencyID,Invoice.ExchangeRate) + Invoice.BankCharges 
      	HReadNext(Invoice,ExpenseAccountID)
      END
      HDeactivateFilter(Invoice)
      
      RESULT cyAmount
      
     type : 458752
   -
     name : RefreshSort
     internal_properties : 
     procedure_id : 1659525142468946450
     type_code : 14
     code : |1-
      // Summary: Refreshes the sort performed in the looper
      // Syntax:
      //refreshSort (<sLooper> is string, <nCurrentSort>, <nSortType>)
      //
      // Parameters:
      //	sLooper (UNICODE string): Looper to sort
      // 	nCurrentSort: <specify the role of nCurrentSort>
      // 	nSortType: <specify the role of nSortType>
      // Return value:
      // 	None
      //
      PROCEDURE RefreshSort(LOCAL sLooper is string, nCurrentSort, nSortType)
      
      // Variable
      sCondition is string
      
      SWITCH nCurrentSort
      	CASE SORT_DATE:
      		sCondition = "ATT_DATE_VALUE"
      	CASE SORT_AMOUNT:
      		sCondition = "ATT_AMOUNT_VALUE"
      	OTHER CASE:
      		Error(<§$0006§>)
      END
      
      SWITCH nSortType
      	CASE SORT_ASCENDING:
      		sCondition = "+" + sLooper+"."+sCondition
      	CASE SORT_DESCENDING:
      		sCondition = "-" + sLooper+"."+sCondition
      	OTHER CASE:
      		Error(<§$0007§>)
      END
      
      LooperSort(sCondition)
     type : 458752
   -
     name : RefreshSynthesis
     internal_properties : 
     procedure_id : 1659525142469011986
     type_code : 14
     code : |1+
      // Summary: Refreshes the preview in synthesis
      // Syntax:
      // RefreshSynthesis ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE RefreshSynthesis()
      
      // Variable
      dStartDate is date = gdCurrentDate ; dStartDate..Day = 1
      dEndDate   is date = dStartDate ; dEndDate..Day = 31
      cyTotalAmount     is currency
      cyRepaidAmount is currency
      
      // Calculates the totals
      QRY_ExpenseAccountList.pEnvelopeID = Null
      QRY_ExpenseAccountList.pStartDate   = dStartDate
      QRY_ExpenseAccountList.pEndDate     = dEndDate
      
      // Runs the query
      IF HExecuteQuery(QRY_ExpenseAccountList) = False THEN
      	Error(HErrorInfo())
      	Close()
      END
      
      // Calculates the total amount and the repaid amount
      FOR EACH QRY_ExpenseAccountList 
      	// Cumulates the total amount
      	cyTotalAmount += AmountAccordingToReference(QRY_ExpenseAccountList.Amount, QRY_ExpenseAccountList.CurrencyID,QRY_ExpenseAccountList.ExchangeRate)
      	
      	// If the bill was reimbursed, it is cumulated to the total of reimbursed bills
      	IF QRY_ExpenseAccountList.Status = STATUS_REIMBURSED THEN
      		cyRepaidAmount += AmountAccordingToReference(QRY_ExpenseAccountList.Amount, QRY_ExpenseAccountList.CurrencyID, QRY_ExpenseAccountList.ExchangeRate)
      	END	
      END
      
      
      
     type : 458752
   -
