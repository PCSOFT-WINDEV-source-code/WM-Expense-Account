      //
      PROCEDURE getImageAccordingToStatus(LOCAL nStatusValue)
      
      SWITCH nStatusValue
      	CASE STATUS_TO_SEND:
      		RESULT IMG_WAIT_SENDING
      	CASE STATUS_PENDING:
      		RESULT IMG_WAITING_FOR_REIMBURSEMENT
      	CASE STATUS_REFUSED:
      		RESULT IMG_REFUSED
      	CASE STATUS_REIMBURSED:
      		RESULT IMG_REIMBURSED
      	OTHER CASE:
      		RESULT ""
      END
