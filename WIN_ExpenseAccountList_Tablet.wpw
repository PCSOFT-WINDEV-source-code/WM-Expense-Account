        internal_properties : 
        code : |1+
         
         
         bDeletion is boolean = False
         
         
         IF YesNo(<§$0013§>) = Yes THEN
         	
         	HReadSeekFirst(Invoice,InvoiceID,LOOP_EXPENSE_ACCOUNT.ATT_ID)
         	IF HFound(Invoice) THEN
         		
         		//Delete the photo files from disk then delete them from the database
         		DeletePhotoFiles(LOOP_EXPENSE_ACCOUNT.ATT_ID)
         		
         		HDelete(Invoice)
         		bDeletion = True	
         	END
         	
         	//	 Refreshes the list of expense accounts
         	IF bDeletion THEN
         		// Refreshes the list of expense accounts
         		IF bDeletion THEN
         			RefreshAll()
         		END
         		
         		RESULT False
         	END
         	
         	
         ELSE
         	
         	RESULT False
         	
         END
