       -
         internal_properties : CAAAAAgAAACfHvyC+ypGezqEGJcnjn9RAIc8mD49InYfY7pE3yb6Y0g0zaSgCAaliUolR+cOJm3u7Zu6Kz9m6XyXjCLmSbcVNYsaU1g6T8zF8S9otntx85AOEtOxKUReZtde2yQi76jeG6B3BdFafuHbXynN4KgpBn17ncXS6aCGxobPEW6ZPxy5bYufGbO70VHKLB5PS8Di4YSbjcz0cPk5iFG8PKTp1vucfxGIIwOuiwUoxKP7MUenttBVeeE8Mek=
         code : |1+
          
          // Variable
          dStartDate is date = Today()
          dEndDate   is date = Today()
          
          // Correct input?
          IF EDT_REQUEST_NAME ~= "" THEN
          	Error(<§$0000§>)
          	RETURN
          END
          
          ////Test removed >> ability to enter bills with identical names
          //// Does the envelope already exist?
          //HReadSeekFirst(Envelope,Name,EDT_REQUEST_NAME)
          //IF HFound(Envelope) THEN
          //	Error("This expense account already exists")
          //	RETURN
          //END
          
          // Adds the envelope
          HReset(Expenses)
          Expenses.Name        = EDT_REQUEST_NAME
          Expenses.StartDate  = dStartDate
          Expenses.EndDate    = dEndDate
          Expenses.StartYear = dStartDate..Year
          Expenses.StartMonth  = dStartDate..Month
          Expenses.EndYear   = dEndDate..Year
          Expenses.EndMonth    = dEndDate..Month
          Expenses.InternalRef = GetExpenseAccountID()
          
          // Adds
          IF HAdd(Expenses) = True THEN
          	
          	MyWindow..ReturnedValue = Expenses.ExpenseAccountID
          	Close()
          	
          ELSE
          	Error(HErrorInfo())
          END
          
