     code : |1-
      PROCEDURE WIN_CategoryEdit(gnCategoryID is int = CS_NO_VALUE)
      
      
      //Display the category, if we want to modify
      IF gnCategoryID <> CS_NO_VALUE THEN
      	
      	HReadSeekFirst(Category,CategoryID,gnCategoryID)
      	SWITCH Nation()
      		CASE nationFrench:
      			EDT_CATEGORY_NAME = Category.Name
      			
      		OTHER CASE:
      			EDT_CATEGORY_NAME = Category.NameUS
      	END
      	
      	gsListKeywords is string = pTools.gaaCategories[Category.CategoryID]
      	FOR EACH string sKeyWord of gsListKeywords SEPARATED BY CR
      		LooperAdd(LOOP_Keywords, sKeyWord)
      	END
      			
      END
      
      IF LOOP_Keywords..Occurrence = 0 THEN
      	LOOP_Keywords..Visible = False
      END
      
      MyWindow..ReturnedValue = gnCategoryID
      SIPVisible(True)
   -
     code : |1+
      
      SetFocus(EDT_CATEGORY_NAME)
