  options :
   -
     name : OPT_OK
     identifier : 0x2707d141d5aea44e
     internal_properties : 
     code_elements :
      type_code : 26
      p_codes :
       -
         internal_properties : CAAAAAgAAAArHkI0NynrkhWLUeOP+25nXXXczfEeNzyZ7PV1EslwUev7VRLmK5hShImq/LzO9QZ0NmZhiyo5MXb49hOuxRHrDb3zmVrCSAzDynHwJlMgbjqN3xUV0vZiZGdqHDYa4nF7ljd9nOufKQhjyg1OEPjgZCSokexWlgtjKYPOmn4O59CY8n5fX5ekb99NcOptzqPlnKrOr8lWfE+8D45EeQkzXAI3QgPhN1tKlEanKYGrin78aGYkTeUC0tkpHvCERknuABw=
         code : |1+
          
          
          // Correct input?
          IF EDT_CATEGORY_NAME ~= "" THEN
          	Error(<§$0000§>)
          	RETURN
          END
          
          //in this case, a new category is added
          IF gnCategoryID = CS_NO_VALUE THEN
          	
          	// Does the category already exists?
          	SWITCH Nation()
          		CASE nationFrench:
          			HReadSeekFirst(Category,Name,EDT_CATEGORY_NAME)
          			IF HFound(Category) THEN
          				Error(<§$0001§>)
          				RETURN
          			END
          			
          		OTHER CASE:
          			HReadSeekFirst(Category,NameUS,EDT_CATEGORY_NAME)
          			IF HFound(Category) THEN
          				Error(<§$0001§>)
          				RETURN
          			END	
          	END
          	
          END
          
          IF gnCategoryID = CS_NO_VALUE THEN
          	HReset(Category)
          END
          
          // Adds the category
          Category.Name   = EDT_CATEGORY_NAME
          Category.NameUS = EDT_CATEGORY_NAME
          
          
          bSuccessful is boolean 
          // Add or modify
          IF gnCategoryID = CS_NO_VALUE THEN
          	
          	bSuccessful = HAdd(Category)
          	
          ELSE
          	
          	bSuccessful = HModify(Category)
          	
          END
          
          IF bSuccessful = True THEN 
          	arrListNewKeywords is array of string
          	FOR ALL ROW of LOOP_Keywords
          		ArrayAdd(arrListNewKeywords, LOOP_Keywords.ATT_Keyword)
          	END
          	
          	AssociateKeywordCategory(arrListNewKeywords, Category.CategoryID)
          	
          	// Selects the new one
          	MyWindow..ReturnedValue = Category.CategoryID
          	
          	// Closes the window
          	Close()
          	
          ELSE
          	Error(HErrorInfo())
          END
         type : 32
