          // Correct input?
          IF EDT_PAYMENTTYPE_CAPTION ~= "" THEN
          	Error(<§$0003§>)
          	RETURN
          END
          
          // Adds the type of payment
          HReset(PaymentType)
          PaymentType.Caption   = EDT_PAYMENTTYPE_CAPTION
          PaymentType.CaptionUS = EDT_PAYMENTTYPE_CAPTION
          
          // Adds
          IF HAdd(PaymentType) = True THEN
          	
