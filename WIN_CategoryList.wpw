  procedures :
   -
     name : RefreshCategoryList
     internal_properties : 
     procedure_id : 1659525073742244707
     type_code : 14
     code : |1-
      // Summary: Refreshes the list of categories
      // Syntax:
      //RefreshCategoryList (<nIDSelectedCategory>)
      //
      // Parameters:
      // 	nSelectedCategoryID: <specify the role of nSelectedCategoryID>
      // Return value:
      // 	None
      //
      PROCEDURE RefreshCategoryList(LOCAL nSelectedCategoryID)
      
      // Variable
      sSearchItem is string
      nSubscript           is int = 0
      
      // Reset
      LooperDeleteAll(LOOP_CATEGORIES)
      
      // Defines the search item
      SWITCH Nation()
      	CASE nationFrench:
      		sSearchItem = Category.Name..Name
      	OTHER CASE:
      		sSearchItem = Category.NameUS..Name
      END
      
      // Displays the categories
      HReadFirst(Category,sSearchItem)
      WHILE NOT HOut(Category)
      	
      	// Fills the looper
      	IF ExtractString(HInfoMemo(Category,Picto),3) > 0 THEN
      		nSubscript = LooperAddLine(LOOP_CATEGORIES,Category.CategoryID, {Category..Name + "." + sSearchItem,indItem}, False, Category.Picto)
      	ELSE
      		nSubscript = LooperAddLine(LOOP_CATEGORIES,Category.CategoryID, {Category..Name + "." + sSearchItem,indItem}, False, IMG_Various)
      	END
      	
      	// Category to select?
      	IF Category.CategoryID = nSelectedCategoryID THEN
      		LOOP_CATEGORIES[nSubscript].ATT_CHECK = True
      		MyWindow..ReturnedValue = nSelectedCategoryID
      	END
      	
      	// Next
      	HReadNext(Category, sSearchItem)
      END
     type : 458752
   -
     name : CategorySelect
     internal_properties : 
     procedure_id : 1659525073742310243
     type_code : 14
     code : |1+
      // Summary: Selects a category in the list
      // Syntax:
      //CategorySelect (<nCategoryID>)
      //
      // Parameters:
      //	nCategoryID: ID of the category
      // Return value:
      // 	None
      //
      PROCEDURE CategorySelect(LOCAL nCategoryID)
      
      // Selects the category
      IF nCategoryID <> CS_NO_VALUE THEN
      	
      	// Browse the looper
      	FOR ALL ROW of LOOP_CATEGORIES
      		IF LOOP_CATEGORIES.ATT_ID = nCategoryID THEN
      			LOOP_CATEGORIES.ATT_CHECK = True
      			RETURN
      		END
      	END
      	
      END
      
      
     type : 458752
