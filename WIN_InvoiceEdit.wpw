   -
     name : invoiceDisplay
     internal_properties : 
     procedure_id : 1659525039376927669
     type_code : 14
     code : |1-
      // Summary: Displays an expense account
      // Syntax:
      //invoiceDisplay (<nInvoiceID> is int)
      //
      // Parameters:
      //	nInvoiceID (integer): Identifier of the expense account
      // Return value:
      // 	None
      //
      PROCEDURE invoiceDisplay(LOCAL nInvoiceID is int)
      
      // Positions on the expense account
      HReadSeekFirst(Invoice,InvoiceID,nInvoiceID)
      IF HFound(Invoice) THEN
      	
      	// Reset
      	ArrayDeleteAll(garrContrib)
      	
      	// Displays the information
      	FileToScreen(WIN_InvoiceEdit, Invoice)
      	
      	IF Val(Invoice.VATAmount2) > 0  THEN
      		gnCurrentyMode = MODE_VAT_SECONDARY
      		mlzAddLine(MZ_VAT)	
      		MZ_VAT[3].EDT_SECONDARY_VAT = Invoice.VATAmount2
      		MZ_VAT[3].EDT_SECONDARY_VAT_RATE = Invoice.VATRate2
      		
      		// Single VAT or more VAT?
      		IF Val(Invoice.VATAmount3) > 0  THEN
      			mlzAddLine(MZ_VAT)	
      			MZ_VAT[4].EDT_SECONDARY_VAT = Invoice.VATAmount3
      			MZ_VAT[4].EDT_SECONDARY_VAT_RATE = Invoice.VATRate3
      			//Up to 3 VATs
      			BTN_ADD_VAT..Visible = False
      		END
      		
      	ELSE
      		gnCurrentyMode = MODE_VAT_UNIQUE
      		BTN_ADD_VAT..BackgroundImage = IMG_PLUS
      		IF MZ_VAT..Occurrence > 2 THEN
      			mlzDelete(MZ_VAT,3)
      		END
      	END
      	
      	// Displays the currency
      	SetCurrency(Invoice.CurrencyID)
      	
      	// Displays the category
      	SetCategory(Invoice.CategoryID)
      	
      	// Customer
      	SetCustomer(Invoice.CustomerID)
      	
      	// Payment
      	SetPayment(Invoice.PaymentTypeID)
      	
      	// Project
      	SetProject(Invoice.ProjectID)
      	
      	// Status
      	SetStatus(Invoice.Status)
      	
      	// Envelope
      	SetExpenseAccount(Invoice.ExpenseAccountID)
      	
      	// Photo?
      	ArrayDeleteAll(gtabPhotos)
      	ArrayDeleteAll(garrDelPhotos)
      	HFilter(Photos,InvoiceID,Invoice.InvoiceID)
      	HReadFirst(Photos,InvoiceID)
      	WHILE NOT HOut(Photos)
      		stAPhoto is stPhoto
      		stAPhoto.nID = Photos.PhotosID
      		stAPhoto.sPath = Photos.Path
      		Add(gtabPhotos,stAPhoto)
      		HReadNext(Photos,InvoiceID)
      	END
      	HDeactivateFilter(Photos)
      	
      	DisplayNbProofs()
      	IF gtabPhotos..Occurrence > 0 THEN
      		gnCurrentSubscript = 1
      	ELSE
      		gnCurrentSubscript = 0	
      	END
      	
      	// Contributors?
      	HFilter(Invoice_Contributors,InvoiceID,Invoice.InvoiceID)
      	HReadFirst(Invoice_Contributors,InvoiceID)
      	WHILE NOT HOut(Invoice_Contributors)
      		Add(garrContrib,Invoice_Contributors.ContributorsID)
      		HReadNext(Invoice_Contributors)
      	END
      	HDeactivateFilter(Invoice_Contributors)
      	
      	SetContributor(garrContrib)
      	
      	// Possibility of deletion
      	BTN_DELETE..Visible = True
      	
      	
      	
      	sSpecialCaption is string
      	nSpecialType is int
      	GetSpecialAccordingToCategory(Invoice.CategoryID,sSpecialCaption, nSpecialType)
      	// Special item?
      	IF sSpecialCaption ~= "" THEN
      		SetSpecial(CS_NO_VALUE)
      	ELSE
      		SetSpecial(Invoice.CategoryID, Invoice.SpecialValue, nSpecialType)	
      	END
      	
      	// Refreshes the height
      	RefreshCommentHeight()
      END
     type : 458752
   -
     name : invoiceReset
     internal_properties : 
     procedure_id : 1659525039376993205
     type_code : 14
     code : |1-
      // Summary: Reinitializes an expense account with the default values
      // Syntax:
      //invoiceReset ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE invoiceReset()
      
      // Reinitializes the controls
      EDIT_IOT = 0
      EDT_VAT     = 0
      EDT_Date    = Today()
      
      // Default values
      SetCategory(CS_NO_VALUE)
      SetCustomer(CS_NO_VALUE)
      SetPayment(gnPaymentTypeID)
      SetProject(CS_NO_VALUE)
      SetStatus(STATUS_TO_SEND)
      SetCurrency(GetDefaultCurrency())
      SetSpecial(CS_NO_VALUE)
      SetExpenseAccount(gnEnvelopeID)
      
      EDT_Date = pTools.GetDefaultDate()
      
      STC_ChooseProofs = StringBuild(<§$001c§>)
      ArrayDeleteAll(gtabPhotos)
      ArrayDeleteAll(garrDelPhotos)
      ArrayDeleteAll(garrContrib)
      SetContributor(GetDefaultContributors())
      
      
      // No deletion is possible, it's a new expense account
      BTN_DELETE..Visible = False
      
      RefreshCommentHeight()
     type : 458752
   -
     name : SetCategory
     internal_properties : 
     procedure_id : 1659525039377058741
     type_code : 14
     code : |1-
      // Summary: Positions the category
      // Syntax:
      //SetCategory (<nCategoryID>)
      //
      // Parameters:
      // 	nCategoryID: Identifier of the category
      // Return value:
      // 	None
      //
      PROCEDURE SetCategory(LOCAL nCategoryID)
      
      gnCategoryID = nCategoryID
      
      // Existing value?
      IF nCategoryID <> CS_NO_VALUE THEN
      	HReadSeekFirst(Category,CategoryID,nCategoryID)
      	IF HFound(Category) THEN
      		SWITCH Nation()
      			CASE nationFrench:
      				STC_ChooseCategory = Category.Name
      			OTHER CASE:
      				STC_ChooseCategory = Category.NameUS
      		END
      		
      		// Displays the special according to the selected category
      		IF Category.SpecialCaption ~= "" THEN
      			SetSpecial(CS_NO_VALUE)
      		ELSE
      			SetSpecial(nCategoryID)	
      		END	
      	ELSE
      		STC_ChooseCategory = ""	
      	END
      ELSE
      	STC_ChooseCategory = ""
      END
     type : 458752
   -
     name : SetCustomer
     internal_properties : 
     procedure_id : 1659525039377124277
     type_code : 14
     code : |1-
      // Summary: Positions the customer
      // Syntax:
      //SetCustomer (<nCustomerID>)
      //
      // Parameters:
      // 	nCustomerID: Identifier of he customer
      // Return value:
      // 	None
      //
      PROCEDURE SetCustomer(LOCAL nCustomerID)
      
      gnCustomerID = nCustomerID
      
      // Existing value?
      IF nCustomerID <> CS_NO_VALUE THEN
      	HReadSeekFirst(Customer,CustomerID,nCustomerID)
      	IF HFound(Customer) THEN
      		STC_CustomerChoice = Customer.Name
      	ELSE
      		STC_CustomerChoice = ""	
      	END
      ELSE
      	//If a single customer in the database, take this one!
      	IF HNbRec(Customer) = 1 THEN
      		HReadFirst(Customer)
      		STC_CustomerChoice = Customer.Name
      	ELSE
      		STC_CustomerChoice = ""
      	END
      	
      END
     type : 458752
   -
     name : SetStatus
     internal_properties : CAAAAAgAAAC1QUYkrN+ZWGkOCAFFIaHnp+4is+1ORSJOSWo0/isld1NPsV4u4HfVyLwLX/xoifH46kEv1+nVr9pQbj4K90tqDAjUezQ9Cx5vh/NExKwmh9UN8Y9OmwPACo3GOUjmTaKb3JugtD56EPqCrSMgHKRiqkUizegVKRFvWHW06TUCvs//1Fd5IWAXrnDdTurW4dmBh4msEIkyh58Sth+eD8RBkJ7zGyKoKk6UXnMcqDAsF8lkzUK+qNf1wBozH3e57+OMl+Dd9s09rKGTtTAfcGnSSjkLIgY=
     procedure_id : 1659525039377189813
     type_code : 14
     code : |1+
      // Summary: Positions the status
      // Syntax:
      //SetStatus (<nStatusValue>)
      //
      // Parameters:
      //	nStatusValue: Status value
      // Return value:
      // 	None
      //
      PROCEDURE SetStatus(LOCAL nStatusValue)
      
      gnStatusValue = nStatusValue
      
      SWITCH gnStatusValue
      	CASE STATUS_TO_SEND:
      		STC_ChooseStatus = <§$0006§>
      	CASE STATUS_PENDING:
      		STC_ChooseStatus = <§$0007§>
      	CASE STATUS_REFUSED:
      		STC_ChooseStatus = <§$0008§>
      	CASE STATUS_REIMBURSED:
      		STC_ChooseStatus = <§$0009§>
      	OTHER CASE:
      		STC_ChooseStatus = ""
      END
