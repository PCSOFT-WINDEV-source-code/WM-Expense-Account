     name : SetCurrency
     internal_properties : 
     procedure_id : 1659525039377517493
     type_code : 14
     code : |1-
      // Summary: Positions the currency
      // Syntax:
      //SetCurrency (<nIDCurrency>)
      //
      // Parameters:
      //	nCurrencyID: Identifier of the currency
      // Return value:
      // 	None
      //
      PROCEDURE SetCurrency(LOCAL nCurrencyID)
      
      gnCurrencyID = nCurrencyID
      
      // Displays the currency
      IF nCurrencyID <> CS_NO_VALUE THEN
      	HReadSeekFirst(LocalCurrency,CurrencyID,nCurrencyID)
      	IF HFound(LocalCurrency) THEN
      		
      		STC_ChooseCurrency = LocalCurrency.Symbol	
      		IF LocalCurrency.Symbol = "$" THEN
      			EDIT_IOT..InputMask = "+9 999 999 999,99 $" + LocalCurrency.Symbol
      			EDT_VAT..InputMask = "+99 999,99 $" + LocalCurrency.Symbol
      			
      			IF gnCurrentyMode = MODE_VAT_SECONDARY THEN
      				MZ_VAT[3].EDT_SECONDARY_VAT..InputMask = "+99 999,99 $" + LocalCurrency.Symbol
      				IF MZ_VAT..Occurrence = 4 THEN
      					MZ_VAT[4].EDT_SECONDARY_VAT..InputMask = "+99 999,99 $" + LocalCurrency.Symbol
      				END
      			END
      			
      		ELSE
      			EDIT_IOT..InputMask = "+9 999 999 999,99 " + LocalCurrency.Symbol[[ TO 3]]	
      			EDT_VAT..InputMask = "+99 999,99 " + LocalCurrency.Symbol
      			
      			IF gnCurrentyMode = MODE_VAT_SECONDARY THEN
      				MZ_VAT[3].EDT_SECONDARY_VAT..InputMask = "+99 999,99 " + LocalCurrency.Symbol
      				IF MZ_VAT..Occurrence = 4 THEN
      					MZ_VAT[4].EDT_SECONDARY_VAT..InputMask = "+99 999,99 " + LocalCurrency.Symbol
      				END
      			END
      			
      		END
      		
      		
      		
      		//If the selected currency is not the reference currency, ask for the exchange rate
      		IF LocalCurrency.Reference = False THEN
      			IF MZ_Localcurrency..Occurrence = 1 THEN
      				mlzAddLine(MZ_Localcurrency)
      				mlzAddLine(MZ_Localcurrency)
      			END
      			
      			
      			MZ_Localcurrency[2].STC_DelCap = "1 "+ pManagingValues.gsSymbolReferenceCurrency + " ="
      			IF LocalCurrency.Symbol = "$" THEN
      				MZ_Localcurrency[2].EDT_Del..InputMask = "+999 999,000000000 $" + LocalCurrency.Symbol
      			ELSE
      				MZ_Localcurrency[2].EDT_Del..InputMask = "+999 999,000000000 " + LocalCurrency.Symbol
      			END
      			
      			MZ_Localcurrency[3].STC_DelCap = <§$0016§>
      			
      			IF pManagingValues.gsSymbolReferenceCurrency = "$" THEN
      				MZ_Localcurrency[3].EDT_Del..InputMask  = "+999 999,99 $" + pManagingValues.gsSymbolReferenceCurrency
      			ELSE
      				MZ_Localcurrency[3].EDT_Del..InputMask  = "+999 999,99" + pManagingValues.gsSymbolReferenceCurrency
      			END
      			
      			//Dipslay the convertion amount in the reference currency
      			MZ_VAT[1]..Height = CoordinateEditorToScreen(60)
      			
      			IF Invoice.ExchangeRate <> 0 THEN
      				MZ_Localcurrency[2].EDT_Del = Invoice.ExchangeRate
      			ELSE
      				MZ_Localcurrency[2].EDT_Del = 1
      			END
      			STC_Conversion = "(" + NumToString(Round(EDIT_IOT / MZ_Localcurrency[2].EDT_Del, 2),",2fS") + [SPC] + pManagingValues.gsSymbolReferenceCurrency + ")"
      			STC_Conversion..Visible = True
      			
      			MZ_Localcurrency[3].EDT_Del = Invoice.BankCharges
      			
      		ELSE
      			IF MZ_Localcurrency..Occurrence > 1 THEN
      				
      				MZ_Localcurrency[2].EDT_Del = 0
      				MZ_Localcurrency[3].EDT_Del = 0
      				
      				mlzDelete(MZ_Localcurrency,3)
      				mlzDelete(MZ_Localcurrency,2)
      				
      				MZ_VAT[1]..Height = MZ_VAT[2]..Height
      				STC_Conversion..Visible = False
      				
      			END
      		END
      	ELSE
      		Error(<§$000f§>)
      	END
      ELSE
      	STC_ChooseCurrency = ""
      END
