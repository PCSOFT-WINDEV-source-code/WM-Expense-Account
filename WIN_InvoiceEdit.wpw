      // Summary: Refreshes the GUI according to the VAT
      // Syntax:
      // RefreshAccordingToVAT ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE RefreshAccordingToVAT()
      
      SWITCH gnCurrentyMode 
      
      	CASE MODE_VAT_UNIQUE 
      		IF MZ_VAT..Occurrence > 2 THEN
      			mlzDelete(MZ_VAT,3)
      		END
      		
      	CASE MODE_VAT_SECONDARY
      		mlzAddLine(MZ_VAT)
      		
      		IF LocalCurrency.Symbol = "$" THEN
      			MZ_VAT[3].EDT_SECONDARY_VAT..InputMask = "+99 999,99 $" + LocalCurrency.Symbol
      		ELSE
      			MZ_VAT[3].EDT_SECONDARY_VAT..InputMask = "+99 999,99 " + LocalCurrency.Symbol
      		END
      		
      	CASE MODE_THREE_VAT
      		mlzAddLine(MZ_VAT)
      		
      		IF LocalCurrency.Symbol = "$" THEN
      			MZ_VAT[4].EDT_SECONDARY_VAT..InputMask = "+99 999,99 $" + LocalCurrency.Symbol
      		ELSE
      			MZ_VAT[4].EDT_SECONDARY_VAT..InputMask = "+99 999,99 " + LocalCurrency.Symbol
      		END
      		
      END
