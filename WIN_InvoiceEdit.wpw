   -
     name : invoiceSave
     internal_properties : 
     procedure_id : 1659525039377451957
     type_code : 14
     code : |1-
      // Summary: Saves the expense account
      // Syntax:
      //invoiceSave ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE invoiceSave()
      
      // New expense account?
      IF gnExpenseAccountID = -1 THEN
      	HReset(Invoice)
      ELSE
      	HReadSeekFirst(Invoice,InvoiceID,gnExpenseAccountID)
      	IF HFound(Invoice) = False THEN
      		Error(<§$000c§>)
      		Close()
      	END
      END
      
      // Specifies the values
      ScreenToFile(WIN_InvoiceEdit,Invoice)
      
      
      Invoice.CategoryID    = gnCategoryID
      Invoice.CustomerID       = gnCustomerID
      Invoice.CurrencyID       = gnCurrencyID
      SetDefaultCurrency(gnCurrencyID)
      
      
      Invoice.ProjectID       = gnProjectID
      Invoice.VATAmount1    = Round(EDT_VAT,2)
      Invoice.Status         = gnStatusValue
      Invoice.VATRate1       = EDT_VAT_RATE
      Invoice.PaymentTypeID = gnPaymentTypeID
      Invoice.ExpenseAccountID    = gnEnvelopeID
      
      // VAT mode?
      IF gnCurrentyMode = MODE_VAT_UNIQUE THEN
      	Invoice.VATAmount2    = 0
      	Invoice.VATRate2       = 0
      	Invoice.VATAmount3    = 0
      	Invoice.VATRate3      = 0
      ELSE
      	Invoice.VATAmount2    = Round(MZ_VAT[3].EDT_SECONDARY_VAT,2)
      	Invoice.VATRate2       = MZ_VAT[3].EDT_SECONDARY_VAT_RATE	
      	IF MZ_VAT..Occurrence = 4 THEN
      		Invoice.VATAmount3    = Round(MZ_VAT[4].EDT_SECONDARY_VAT,2)
      		Invoice.VATRate3      = MZ_VAT[4].EDT_SECONDARY_VAT_RATE	
      	ELSE
      		Invoice.VATAmount3    = 0
      		Invoice.VATRate3      = 0
      	END
      END
      
      IF MZ_Localcurrency..Occurrence < 2 THEN
      	Invoice.ExchangeRate = 0
      	Invoice.BankCharges = 0
      ELSE
      	Invoice.ExchangeRate = MZ_Localcurrency[2].EDT_Del
      	Invoice.BankCharges =  MZ_Localcurrency[3].EDT_Del
      END
      
      // Special item?
      IF MZ_General..Occurrence < 5 THEN
      	Invoice.SpecialValue = ""
      ELSE
      	IF MZ_General[4].EDT_SPECIAL_NUM..Visible = True THEN 
      		Invoice.SpecialValue = MZ_General[4].EDT_SPECIAL_NUM
      	END
      	
      	IF MZ_General[4].EDT_SPECIAL_TEXT..Visible = True THEN 
      		Invoice.SpecialValue = NoSpace(MZ_General[4].EDT_SPECIAL_TEXT)	
      	END
      END
      
      // Adds or modifies
      IF gnExpenseAccountID <> -1 THEN
      	HModify(Invoice)
      ELSE
      	HAdd(Invoice)
      END
      
      // Saves the ID
      gnExpenseAccountID = Invoice.InvoiceID
      
      // Process on the photos
      PhotoProcess(True)
      
      // Refreshes the period of the envelope
      IF Invoice.ExpenseAccountID <> CS_NO_VALUE THEN
      	EnvelopeRefreshPeriod(Invoice.ExpenseAccountID)
      END
      
      // Saves the contributors
      HReadSeekLast(Invoice_Contributors,InvoiceID,gnExpenseAccountID)
      WHILE HFound(Invoice_Contributors)
      	HDelete(Invoice_Contributors)
      	HReadPrevious(Invoice_Contributors,InvoiceID)
      END
      
      FOR ALL ELEMENT nContributorID of garrContrib
      	HReset(Invoice_Contributors)
      	Invoice_Contributors.ContributorsID = nContributorID
      	Invoice_Contributors.InvoiceID = gnExpenseAccountID
      	HAdd(Invoice_Contributors)
      END
