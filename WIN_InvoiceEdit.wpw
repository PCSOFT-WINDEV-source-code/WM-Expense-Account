   -
     name : PhotoProcess
     internal_properties : 
     procedure_id : 1659525039377779637
     type_code : 14
     code : |1-
      // Summary: Processes the actions that must be done on the photos
      // Syntax:
      //PhotoProcess (<bSave> is boolean)
      //
      // Parameters:
      //	bSave (boolean): Backup?
      // Return value:
      // 	None
      //
      PROCEDURE PhotoProcess(bSave is boolean)
      
      // Cancel?
      IF bSave = False THEN
      	
      	// The files that will not be used must be deleted
      	FOR EACH ELEMENT stAPhoto of gtabPhotos
      		HReadSeekFirst(Photos,Path,stAPhoto.sPath)
      		IF HFound(Photos) = False THEN
      			fDelete(stAPhoto.sPath)
      		END
      	END
      	
      	FOR EACH ELEMENT sFile of garrDelPhotos
      		HReadSeekFirst(Photos,Path,sFile)
      		IF HFound(Photos) = False THEN
      			fDelete(sFile)
      		END	
      	END
      ELSE
      	// Deletes the photos that are no longer requested
      	FOR ALL ELEMENT sFile of garrDelPhotos
      		HReadSeekFirst(Photos,Path,sFile)
      		IF HFound(Photos) = True THEN
      			IF Photos.InvoiceID = gnExpenseAccountID THEN
      				fDelete(sFile)
      			END
      		END	
      	END
      	
      	// Add the new ones
      	FOR ALL ELEMENT stAPhoto of gtabPhotos
      		
      		HReadSeekFirst(Photos,Path,stAPhoto.sPath)
      		IF HFound(Photos) = False THEN
      			HReset(Photos)
      			Photos.Path  = stAPhoto.sPath
      			Photos.InvoiceID = gnExpenseAccountID
      			HAdd(Photos)
      		END
      	END
      END
