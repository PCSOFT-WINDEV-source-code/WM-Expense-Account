      //
      PROCEDURE SetProject(LOCAL nProjectID)
      
      gnProjectID = nProjectID
      
      // Existing value?
      IF nProjectID <> CS_NO_VALUE THEN
      	HReadSeekFirst(Project,ProjectID,nProjectID)
      	IF HFound(Project) THEN
      		STC_ChooseProject = Project.Name
      	ELSE
      		STC_ChooseProject= ""	
      	END
      ELSE
      	
      	//If a single project in the database, take this one!
      	IF HNbRec(Project) = 1 THEN
      		HReadFirst(Project)
      		STC_ChooseProject = Project.Name
      	ELSE
      		STC_ChooseProject = ""
      	END
      	
      END
      
