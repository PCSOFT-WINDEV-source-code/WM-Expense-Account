     procedure_id : 1659525039377386421
     type_code : 14
     code : |1+
      // Summary: Checks the input of the expense account
      // Syntax:
      //[ <Result> = ] invoiceCheck ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	boolean: // 	None
      //
      PROCEDURE invoiceCheck()
      
      // Checks the category
      IF gnCategoryID = CS_NO_VALUE THEN
      	Error(<§$000a§>)
      	RESULT False
      END
      
      // Amount?
      IF EDIT_IOT = 0 THEN
      	Error(<§$000b§>)
      	
      	SetFocus(EDIT_IOT)
      	RESULT False
      END
      
      // Type of payment
      IF gnPaymentTypeID = CS_NO_VALUE THEN
      	Error(<§$0010§>)
      	RESULT False
      END
      
      //if gnEnvelopeID = CS_NO_VALUE THEN
      //	Error("Select an expense account")
      //	RESULT False
      //END
      
      RESULT True
      
